  <!-- INTRO -->
  <section class="intro">

    <div class="row">

      <div class="medium-12 columns">

        <h1>Designer &amp; Developer</h1>
        <p>My name is Michael Elliott.<br>I am from Salt Lake City, Utah, currently in Seattle. My hobbies include making music, programming, rock climbing, snowboarding, and video games. I have a dog, a cat, and a girlfriend. They are the best.</p>

        <p><a href="#contact" class="button small alert radius">Hit me up</a></p>

        <!-- Start: Slider -->
        <div class="csslider">

          <input type="radio" name="slides" id="slides_1" checked>
          <input type="radio" name="slides" id="slides_2">
          <ul>
            <li><%= image_tag ("headshot.jpg") %></li>
            <li><%= image_tag ("mac.png") %></li>
          </ul>

          <!-- Buttons Control -->
          <div class="navigation">

            <div>
              <label for="slides_1"></label>
              <label for="slides_2"></label>
            </div>
            
          </div><!-- /.navigation -->
          
        </div><!-- /.csslider -->
        <!-- End: Slider -->
        
      </div><!-- /.medium-12 .columns -->

    </div><!-- /.row -->
    
  </section><!-- /.intro -->
  <!-- INTRO -->




